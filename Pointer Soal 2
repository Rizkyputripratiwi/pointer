#include <iostream>
using namespace std;
//Rizky Putri Pratiwi
//G1A021044
//Pointer Nomor 2

int main(){

  		int a, b, c;
		int *p1, *p2, *p3;


  	a = 10 ;
  	b=15;
  	c = 27;
  	p1 = &a;
	p2 = &b;
	*p1 = c;
	a = *p2;
	b = 6;
	p3 = &b;
	p3 = &c;
	*p1 = *p3;
	
	cout<<"a = "<<10<<endl;
	cout<<"b =  "<<b<<endl;
	cout<<"c = "<<c<<endl;
  	cout << "p1 =" << &a<<endl;
  	cout << "p2 =" << &b << endl;
  	cout << "*p1 = " << c << endl;
  	cout << "a = " << *p2 << endl;
  	cout<<"b = "<<6<<endl;
  	cout << "p3 =" << &b <<endl;
  	cout << "*p1 = *p3" << endl;

	return 0;
}
